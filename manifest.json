{
  "version": "0.1.0",
  "createdAt": "2023-06-26T00:00:00.000000Z",
  "expiresAt": "2100-01-01T00:00:00.000000Z",
  "metadata": {
    "name": "Golem Ray",
    "description": "Golem ray webserver",
    "version": "0.0.1"
    },
  "payload": [
    {
      "urls": [
        "http://registry.golem.network/download/{IMAGE_HASH}"
      ],
      "hash": "sha3:{IMAGE_HASH}"
    }
  ],
  "compManifest": {
    "version": "0.1.0",
    "script": {
      "commands": [
        "run /bin/sh -c echo*",
        "run /bin/sh -c mkdir -p /root/.ssh",
        "run /bin/sh -c cat*",
        "run /bin/sh -c service ssh start",
        "run /bin/sh -c ssh*",
        "run /bin/sh -c *"
      ],
      "match": "regex"
    },
    "net": {
      "inet": {
        "out": {
          "protocols": ["https", "tcp"],
          "urls": [
            "tcp://proxy.dev.golem.network:{SSH_TUNNEL_PORT}/",
            "tcp://proxy.dev.golem.network:4000/",
            "tcp://proxy.dev.golem.network:4001/",
            "tcp://proxy.dev.golem.network:4002/",
            "tcp://proxy.dev.golem.network:4003/",
            "tcp://proxy.dev.golem.network:4004/",
            "tcp://proxy.dev.golem.network:4005/",
            "tcp://proxy.dev.golem.network:4006/",
            "https://pypi.dev.golem.network"]
        }
      }
    }
  }
}
